  <script type="text/javascript">
  var Dom = YAHOO.util.Dom,
      Event = YAHOO.util.Event,
      Anim = YAHOO.util.Anim;

  

  /* Follow callback function */
  function followCB() {
    //TPApp.util.anim.swap( 'follow-display', 'follow-confirm' );
    TPApp.util.anim.swap( 'follow-spinner', 'follow-confirm' );
    
    if( Dom.get('num-followers') )
        Dom.get('num-followers').innerHTML = TPApp.util.following.formatInteger(
            TPApp.util.following.parseInteger( Dom.get('num-followers').innerHTML ) + 1
        );

    /* Send message back to toolbar */
    TPApp.util.crossTalk.sendMessage( window.parent, 'followTB', 'http://blog.walteralmeida.com/2010/05/advanced-linq-dynamic-linq-library-add-support-for-contains-extension-.html' );
    
  };
  
  /* Unfollow callback function */
  function unfollowCB() {
    //TPApp.util.anim.swap( 'unfollow-display', 'follow-display' );
    TPApp.util.anim.swap( 'follow-spinner', 'follow-display' );

    if( Dom.get('num-followers') )
        Dom.get('num-followers').innerHTML = TPApp.util.following.formatInteger(
            TPApp.util.following.parseInteger( Dom.get('num-followers').innerHTML ) - 1
        );
    
    /* Send message back to toolbar */
    TPApp.util.crossTalk.sendMessage( window.parent, 'unfollowTB', 'http://blog.walteralmeida.com/2010/05/advanced-linq-dynamic-linq-library-add-support-for-contains-extension-.html' );

  };

  /* crossTalk receiver callback */
  var ctCB = function( msg ) {

    switch( msg ) {

        case "follow":
            TPApp.util.anim.swap( 'follow-display', 'follow-confirm' );
            if( Dom.get('num-followers') )
                Dom.get('num-followers').innerHTML = TPApp.util.following.formatInteger(
                    TPApp.util.following.parseInteger( Dom.get('num-followers').innerHTML ) + 1
                );
            break;
        case "unfollow":
            TPApp.util.anim.hide( 'unfollow-display' );
            TPApp.util.anim.swap( 'follow-confirm', 'follow-display' );
            if( Dom.get('num-followers') )
                Dom.get('num-followers').innerHTML = TPApp.util.following.formatInteger(
                    TPApp.util.following.parseInteger( Dom.get('num-followers').innerHTML ) - 1
                );
            break;
        default:

    }

  };

  Event.onDOMReady( function() {

    /* Set JSONRPC endpoint */
    TPApp.util.jsonrpc.uri = "http://www.typepad.com/services/json-rpc";

    /* Follow user click handler */
    Event.on('follow-button', 'click', function( ev ) {
      Event.stopEvent( ev );
      TPApp.util.anim.swap( 'follow-display', 'follow-spinner' );
      TPApp.util.following.follow( '6p0133ecb036c7970b', followCB );
    });

    /* Unfollow user click handler */
    Event.on('confirm-unfollow', 'click', function( ev ) {
      Event.stopEvent( ev );
      TPApp.util.anim.swap( 'unfollow-display', 'follow-spinner' );
      TPApp.util.following.unfollow( '6p0133ecb036c7970b', unfollowCB );
    });

    /* Watch for (un)follow messages from the blogside toolbar */
    TPApp.util.crossTalk.receiveMessage( "http://www.typepad.com/services/connect/profile_module", ctCB );

  } );
  </script>
